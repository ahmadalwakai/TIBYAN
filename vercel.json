{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "regions": ["fra1"],
  "build": {
    "env": {
      "PRISMA_CLI_BINARY_TARGETS": "rhel-openssl-3.0.x",
      "PRISMA_SKIP_POSTINSTALL_GENERATE": "true"
    }
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "crons": []
}
